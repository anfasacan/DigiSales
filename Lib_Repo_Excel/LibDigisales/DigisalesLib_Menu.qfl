	End If
	Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), "Konfirmasi Login", "" , compatibilityMode.Desktop, ReportStatus.Done)
	wait 1
	Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal").WebButton("Setuju").Click
	wait 2

	Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("DigiSales"), "Login Berhasil", " ", compatibilityMode.Desktop, ReportStatus.Passed)
	wait 2
End Function

