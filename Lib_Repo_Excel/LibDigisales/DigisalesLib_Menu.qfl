		Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), " Gagal Masuk ke Homepage Digisales", " " , compatibilityMode.Desktop, ReportStatus.Failed)	
		Call CloseBrowser()
		Call spReportSave()
