REM ---------------------------- USER Digisales LOGOUT ------------------------------------
Sub captureError()
	If Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal").WebElement("Sign in").Exist(5) Then
		Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), "General Error", " ", compatibilityMode.Desktop, ReportStatus.Failed)
	ElseIf Browser("DigiSalesWebPortal").Page("DigiSales").WebElement("Profile").Exist(5) Then		
		Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("DigiSales"), "General Error", " ", compatibilityMode.Desktop, ReportStatus.Failed)
	End If
	
End Sub


Function DA_Logout(ByVal isError)
	If isError = "1" Then
		Call captureError()
	End If

	wait 1
	If Browser("DigiSalesWebPortal").Page("DigiSales").WebElement("Profile").Exist(10) Then
		Browser("DigiSalesWebPortal").Page("DigiSales").WebElement("Profile").Click
		Browser("DigiSalesWebPortal").Page("DigiSales").Link("keyboard_tab Logout").Click

		If Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal").WebElement("Sign in").Exist(30) Then
			Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), "Berhasil Logout", " ", compatibilityMode.Desktop, ReportStatus.Passed)
			Call CloseBrowser()
			Call spReportSave()
'		
		If isError = "1" Then
			If chr(Environment("TestIteration")) = chr(DataTable.GetRowCount) Then
				Call spReportSave()
				Call ExitActionIteration()
			Else
				Call ExitActionIteration()
			End If
		End If	
	Else 
		Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), "Transaksi Failed", " ", compatibilityMode.Desktop, ReportStatus.Failed)
		Reporter.ReportEvent micFail, "Transaksi Gagal", "Transaksi Gagal"
		Call spReportSave()
		Call CloseBrowser()
		Call ExitActionIteration()
	End If
Else 
	Call CaptureImageUFTV2(Browser("DigiSalesWebPortal").Page("User Login | DigiSalesWebPortal"), "Transaksi Failed", " ", compatibilityMode.Desktop, ReportStatus.Failed)
	Reporter.ReportEvent micFail, "Transaksi Gagal", "Transaksi Gagal"
	Call spReportSave()
	Call CloseBrowser()
	Call ExitActionIteration()
End  If
End Function

'Fungsi Logout Digisales Mobile
Function DA_LogoutMobile(ByVal isError)
	If isError = "1" Then
		Call captureError()
	End If
	
	Do while Browser("DigiSales").Page("HeadBar").WebElement("Back").Exist(1)
		Browser("DigiSales").Page("HeadBar").WebElement("Back").Click
	loop
	
	wait 1
	If Browser("DigiSales").Page("Navbar").WebElement("Profile").Exist(5) Then
		Browser("DigiSales").Page("Navbar").WebElement("Profile").Click
		If Browser("DigiSales").Page("DigiSales Mobile | Profile").WebElement("Alert Whatsapp").Exist(5) Then
			Browser("DigiSales").Page("DigiSales Mobile | Profile").WebButton("Btn Alert Oke").Click
		End If
		wait 2
			Set objkey = CreateObject("WScript.Shell")
			objkey.SendKeys "{PGDN}"
		wait 2	
			Call CaptureImageUFTV2(Browser("DigiSales").Page("DigiSales Mobile | Login"), "Pilih Logout Pada Menu Profile", " ", compatibilityMode.Desktop, ReportStatus.Done)
		wait 1
		Browser("DigiSales").Page("DigiSales Mobile | Profile").WebButton("Logout").Click
		If Browser("DigiSales").Page("DigiSales Mobile | Login").WebButton("Btn Login Mobile").Exist(10) Then
			wait 1
			Call CaptureImageUFTV2(Browser("DigiSales").Page("DigiSales Mobile | Login"), "Berhasil Logout", " ", compatibilityMode.Desktop, ReportStatus.Passed)
			wait 2
			Call CloseBrowser()
			Call spReportSave()
'		
		If isError = "1" Then
			If chr(Environment("TestIteration")) = chr(DataTable.GetRowCount) Then
				Call spReportSave()
				Call ExitActionIteration()
			Else
				Call ExitActionIteration()
			End If
		End If	
	Else 
		Call CaptureImageUFTV2(Browser("DigiSales").Page("DigiSales Mobile | Login"), "Gagal Logout", " ", compatibilityMode.Desktop, ReportStatus.Failed)
		Reporter.ReportEvent micFail, "Gagal Logout", "Gagal Logout"
		Call spReportSave()
		Call CloseBrowser()
		Call ExitActionIteration()
	End If
Else 
	Call CaptureImageUFTV2(Browser("DigiSales").Page("DigiSales Mobile | Login"), "Logout Failed", " ", compatibilityMode.Desktop, ReportStatus.Failed)
	Reporter.ReportEvent micFail, "Logout Gagal", "Logout Gagal"
	Call spReportSave()
	Call CloseBrowser()
	Call ExitActionIteration()
End  If
End Function

