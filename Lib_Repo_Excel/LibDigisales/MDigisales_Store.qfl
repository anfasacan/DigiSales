REM ========= Go to Sub Navbar Menu / Sub Menu Digisales Mobile
Function GoToSubNavbar_Store()
	Dim SubNavbar
	SubNavbar	= DataTable.Value("SUB_NAVBAR" ,dtLocalSheet)
	SubNavbar	= ucase(SubNavbar)
	Select Case SubNavbar
		Case "DEDICATED"
			Browser("DigiSales").Page("NavStore").WebElement("Dedicated").Click
			wait 2
			Call CaptureImageUFTV2(Browser("DigiSales").Page("Dedicated"), "Masuk Ke Menu Dedicated", " " , compatibilityMode.Desktop, ReportStatus.Passed)
			wait 2
		Case "FREE"
			Browser("DigiSales").Page("NavStore").WebElement("Free").Click
			wait 2
			Call CaptureImageUFTV2(Browser("DigiSales").Page("Free"), "Masuk Ke Menu Free", " " , compatibilityMode.Desktop, ReportStatus.Passed)
			wait 2
		Case "KELOLAAN"
			Browser("DigiSales").Page("NavStore").WebElement("Kelolaan").Click
			wait 2
			Call CaptureImageUFTV2(Browser("DigiSales").Page("Kelolaan"), "Masuk Ke Menu Kelolaan", " " , compatibilityMode.Desktop, ReportStatus.Passed)
			wait 2
		Case "PROSPEK"
			wait 2
			Browser("DigiSales").Page("NavStore").WebElement("Prospek").Click
			wait 2
			Call CaptureImageUFTV2(Browser("DigiSales").Page("Prospek"), "Masuk Ke Menu Prospek", " " , compatibilityMode.Desktop, ReportStatus.Passed)
			wait 2
		Case Else
			msgbox SubNavbar & " Tidak Ditemukan" 
	End Select
End Function

